<%- model_class = Estimate -%>
<div class="page-header">
  <h1><%=t 'estimates.edit_title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>

<%= form_for @estimate, url: user_estimate_path(current_user, @estimate) do |f| %>
