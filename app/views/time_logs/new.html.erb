<%- model_class = Task -%>
<div class="page-header">
  <h1><%=t 'time_logs.create_title', :default => [:'helpers.titles.new', 'New %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>
<%= form_for @time_log, url: user_project_task_time_logs_url(current_user, params[:project_id], params[:task_id]), :html => { :class => "task" } do |f| %>
  <%= render :partial => 'form',  locals: { f: f } %>
<% end %>
