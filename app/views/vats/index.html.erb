<p id="notice"><%= notice %></p>

<h1>Listing Vats</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vats.each do |vat| %>
      <tr>
        <td><%= link_to 'Show', vat %></td>
        <td><%= link_to 'Edit', edit_vat_path(vat) %></td>
        <td><%= link_to 'Destroy', vat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vat', new_vat_path %>
