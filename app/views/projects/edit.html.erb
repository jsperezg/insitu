<%- model_class = Project -%>
<div class="page-header">
  <h1><%=t 'projects.edit_title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>
<%= form_for @project, url: user_project_path(current_user, @project) do |f| %>
  <%= render :partial => 'form', locals: { f: f } %>
<% end %>
