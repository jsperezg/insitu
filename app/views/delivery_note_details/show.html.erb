<%- model_class = DeliveryNoteDetail -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:delivery_note_id) %>:</strong></dt>
  <dd><%= @delivery_note_detail.delivery_note_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:service_id) %>:</strong></dt>
  <dd><%= @delivery_note_detail.service_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantity) %>:</strong></dt>
  <dd><%= @delivery_note_detail.quantity %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              delivery_note_details_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_delivery_note_detail_path(@delivery_note_detail), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              delivery_note_detail_path(@delivery_note_detail),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
